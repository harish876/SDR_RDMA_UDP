# Maximum Transmission Unit (MTU) - packet payload size in bytes
# This is the size of each UDP packet payload (not including headers)
# Must be <= 1476 (1500 - 8 UDP header - 16 SDR packet header)

mtu_bytes=4096
packets_per_chunk=32
transfer_id=1
window_size=50
# UDP IP to advertise in CTS (set to this host's reachable address)
udp_server_ip=127.0.0.1
