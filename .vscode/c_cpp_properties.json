{
  "configurations": [
    {
      "name": "macos-clang-x64",
      "includePath": [
        "${workspaceFolder}/**",
        "${workspaceFolder}/infinity/src",
        "${workspaceFolder}/infinity/release/include",
        "${workspaceFolder}/infinity/src/infinity"
      ],
      "defines": [],
      "compilerPath": "/usr/bin/g++",
      "cStandard": "c11",
      "cppStandard": "c++11",
      "intelliSenseMode": "macos-clang-x64",
      "compilerArgs": [
        "-O3",
        "-std=c++0x"
      ],
      "browse": {
        "path": [
          "${workspaceFolder}/infinity/src",
          "${workspaceFolder}/infinity/release/include"
        ],
        "limitSymbolsToIncludedHeaders": true
      }
    }
  ],
  "version": 4
}